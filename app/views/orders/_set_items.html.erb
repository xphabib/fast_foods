        <div class="food-details">
          <%= image_tag("#{food.image}", size: "100x60" ) %>
          <div class="food-name-discount" >
            <%= food.name %> (à§³ <%= food.sales_price %>, <%= set_discount(food.id) %>)
          </div>
        </div>
    <% end %>
<% end %>